    "def fizzbuzz(x):\n",
    "    for num in range(1,x):\n",
    "        if num % 3 == 0 and num % 5 == 0:\n",
    "            print(\"Fizzbuzz\")\n",
    "        elif num % 3 == 0:\n",
    "            print(\"Fizz\")\n",
    "        elif num % 5 == 0:\n",
    "            print(\"Buzz\")\n",
    "        else: print(num)"
