   "source": [
    "def sum_factors(x):\n",
    "    list = []\n",
    "    for num in range(x):\n",
    "        if num % 3 == 0 or num % 5 == 0:\n",
    "            list.append(num)\n",
    "    list_sum = sum(list)\n",
    "    print(list_sum)"
   ]
